public int main() {
    class c2 x;
    x=new c2();
    x.i2=new int[12];
    return x.m2(new c1());
}

public class c1 {
    int c1m1;
    public class c1 m1(int x) {
        this.c1m1=x;
        return this;
    }
}

public class c2 extends c1 {
    int[] i2={100};
    public int m2(class c1 c) {
        return c.m1(this.i2[0]).c1m1;
    }
}

