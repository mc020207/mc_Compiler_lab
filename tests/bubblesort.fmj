public class b1 {
  public int bubbleSort(int[] array, int size) {
  int i=0;
  int temp;

  if (size <= 1) {
    return 0;
  }

  while (i < size - 1) {
    if (array[i] > array[i + 1]) {
      temp = array[i];
      array[i] = array[i + 1];
      array[i + 1] = temp;
    }
    i=i+1;
  }

  return this.bubbleSort(array, size - 1);
  }
}
